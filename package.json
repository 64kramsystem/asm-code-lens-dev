{
    "name": "asm-code-lens",
    "displayName": "ASM Code Lens",
    "version": "1.0.0",
    "publisher": "maziac",
    "description": "A language server that enables code lens, references, hover information and symbol renaming in asssembler files.",
    "author": {
        "name": "Thomas Busse"
    },
    "license": "MIT",
    "keywords": [
        "assembler",
        "references",
        "code lens"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/maziac/asm-code-lens"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "icon": "assets/icon.png",
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "properties": {}
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^5.2.0",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "fast-glob": "^2.2.2",
        "p-queue": "^2.4.2"
    }
}
